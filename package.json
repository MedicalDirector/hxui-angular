    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "^6.1.0",
    "angular-in-memory-web-api": "~0.11.0",
    "array-sort-by": "^1.2.1",
    "core-js": "^3.22.3",
    "highlight.js": "^10.4.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.29.3",
