{
  "name": "@hxui/docs",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "author": "Design @ MD <design@medicaldirector.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MedicalDirector/hxui-angular.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "install-peers": "npm install @angular/animations @angular/cdk @angular/common @angular/compiler @angular/core @angular/forms @angular/platform-browser @angular/platform-browser-dynamic @angular/router @angular/localize @hxui/css @hxui/ng-select @ng-select/ng-option-highlight @ng-select/ng-select array-sort-by jquery lodash lodash.clonedeep moment ngx-mask rxjs selectize ngx-toastr",
    "docs:start": "ng serve docs",
    "docs:build": "ng build docs --configuration production",
    "docs:test": "ng test docs --coverage",
    "hxui:build": "ng build hxui --configuration production",
    "hxui:test": "ng test hxui --coverage",
    "hxui:test:watch": "ng test hxui --watch",
    "postinstall": "ngcc",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "dependencies": {
    "@angular/animations": "^13.3.8",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "^13.3.8",
    "@angular/compiler": "^13.3.8",
    "@angular/core": "^13.3.8",
    "@angular/forms": "^13.3.8",
    "@angular/platform-browser": "^13.3.8",
    "@angular/platform-browser-dynamic": "^13.3.8",
    "@angular/platform-server": "^13.3.8",
    "@angular/router": "^13.3.8",
    "@hxui/css": "^1.5.4",
    "@hxui/ng-select": "^1.1.0",
    "@ng-select/ng-option-highlight": "0.0.7",
    "@ng-select/ng-select": "^8.1.1",
    "angular-in-memory-web-api": "~0.11.0",
    "array-sort-by": "^1.2.1",
    "core-js": "^3.22.5",
    "highlight.js": "^10.4.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.29.3",
    "ngx-highlightjs": "4.1.3",
    "ngx-mask": "^13.1.13",
    "ngx-page-scroll": "^8.0.0",
    "ngx-page-scroll-core": "^8.0.0",
    "ngx-toastr": "^14.3.0",
    "rxjs": "^6.6.7",
    "selectize": "^0.12.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular-eslint/builder": "4.3.1",
    "@angular-eslint/eslint-plugin": "4.3.1",
    "@angular-eslint/eslint-plugin-template": "4.3.1",
    "@angular-eslint/schematics": "4.3.1",
    "@angular-eslint/template-parser": "4.3.1",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/localize": "^13.3.8",
    "@angular/service-worker": "^13.3.8",
    "@babel/core": "^7.17.10",
    "@briebug/jest-schematic": "^4.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/angular": "^6.4.22",
    "@storybook/testing-library": "0.0.11",
    "@testing-library/angular": "^10.11.1",
    "@types/highlightjs": "^9.12.2",
    "@types/jest": "^27.5.0",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.20.51",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.5.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^1.6.2",
    "ng-packagr": "^13.3.1",
    "prettier": "^2.6.2",
    "typescript": "^4.4.4",
    "webpack": "^5.30.0"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
