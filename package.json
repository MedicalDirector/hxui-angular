    "ts-node": "~8.4.1",
    "typescript": "^4.0.8",
    "webpack": "^4.46.0"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
