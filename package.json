{
  "name": "@hxui/docs",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "author": "Design @ MD <design@medicaldirector.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MedicalDirector/hxui-angular.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install-peers": "npm install @angular/animations @angular/cdk @angular/common @angular/compiler @angular/core @angular/forms @angular/platform-browser @angular/platform-browser-dynamic @angular/router @angular/localize @hxui/css @hxui/ng-select @ng-select/ng-option-highlight @ng-select/ng-select array-sort-by jquery lodash lodash.clonedeep moment ngx-mask rxjs selectize ngx-toastr",
    "start:docs": "ng serve docs",
    "build:docs": "ng build docs --prod",
    "test:docs": "ng test docs --code-coverage=true --watch=false --browsers=ChromeHeadless",
    "e2e:docs": "ng e2e docs",
    "build:hxui": "ng build hxui --prod",
    "test:hxui": "ng test hxui --code-coverage=true --watch=false",
    "test:hxui:headless": "ng test hxui --code-coverage=true --watch=false --browsers=ChromeHeadless",
    "postinstall": "ngcc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/platform-server": "^11.2.14",
    "@angular/router": "^11.2.14",
